#include <iostream>
using namespace std;

int main() {
    int start, end;

    // Input the range
    cout << "Enter the starting number: ";
    cin >> start;

    cout << "Enter the ending number: ";
    cin >> end;

    cout << "Numbers in sequence: ";
    for (int i = start; i <= end; i++) {
        cout << i << " ";
    }
    cout << endl;

    return 0;
}
